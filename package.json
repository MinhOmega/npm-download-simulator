{
  "name": "npm-download-simulator",
  "version": "0.0.1",
  "description": "Raises the popularity score on your package through nefarious means.",
  "author": "MinhOmega",
  "license": "MIT",
  "main": "./lib/src/index.js",
  "dependencies": {
    "date-fns": "2.28.0",
    "gaxios": "5.0.0",
    "ora": "5.4.1",
    "prompts": "2.4.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "27.5.0",
    "@types/node": "16.11.7",
    "@types/prompts": "2.0.14",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.14.0",
    "jest": "28.0.3",
    "jest-mock-console": "1.2.3",
    "nock": "13.2.4",
    "nodemon": "^3.1.4",
    "prettier": "2.6.2",
    "semantic-release": "19.0.2",
    "ts-jest": "28.0.0",
    "ts-node": "^10.9.2",
    "typescript": "4.6.4"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node .",
    "test": "jest --coverage --detectOpenHandles",
    "format": "prettier . --write & eslint . --fix",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "dev:cli": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/cli.ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": ["<rootDir>/(coverage|lib).*/"],
    "coverageReporters": ["json-summary", "text", "lcov"]
  },
  "bin": {
    "npm-download-simulator": "./lib/src/cli.js"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**"
  ],
  "keywords": [
    "npm",
    "npmjs",
    "npms",
    "popularity",
    "increaser",
    "spammer",
    "download-simulator",
    "package-stats",
    "cli-tool",
    "typescript",
    "npm-download-simulator"
  ],
  "repository": "https://github.com/MinhOmega/npm-download-simulator.git",
  "bugs": "https://github.com/MinhOmega/npm-download-simulator/issues",
  "homepage": "https://github.com/MinhOmega/npm-download-simulator#readme"
}
